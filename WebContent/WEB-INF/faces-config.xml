<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	
	<application>
		<resource-handler>org.primefaces.component.export.portlet.resourcehandler.ExportResourceHandler</resource-handler>
	</application>
	
	<managed-bean>
		<managed-bean-name>viewDataExportModelBean</managed-bean-name>
		<managed-bean-class>thecat.view.ViewDataExportModelBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>viewDataExportBackingBean</managed-bean-name>
		<managed-bean-class>thecat.view.ViewDataExportBackingBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
</faces-config>
